{
  "priority" : 1,
  "operators": {
    "load": {
       "type": "TableLoad",
       "table": "STOCK"
    },

    "idx_scan_S_I_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(ol_i_id)d,
      "fields": ["S_I_ID"],
      "index": "STOCK_idx_S_I_ID"
    },

    "idx_scan_S_W_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(ol_supply_w_id)d,
      "fields": ["S_W_ID"],
      "index": "STOCK_idx_S_W_ID"
    },

    "intersect": {
      "type": "IntersectPositions"
    },

    "validate" : {
      "type" : "ValidatePositions"
    },
    "update": {
       "type": "PosUpdateScan",
       "data": {
          "S_QUANTITY": %(s_quantity)d,
          "S_ORDER_CNT": %(s_order_cnt)d,
          "S_REMOTE_CNT": %(s_remote_cnt)d,
          "S_YTD": %(s_ytd)d
       }
    },
    "NoOp": {
      "type": "NoOp"
    }
  },
  "edges": [
    ["load", "idx_scan_S_I_ID"], 
    ["load", "idx_scan_S_W_ID"],

    ["idx_scan_S_I_ID", "intersect"], 
    ["idx_scan_S_W_ID", "intersect"],

    ["intersect", "validate"],
    ["validate", "update"],
    ["update","NoOp"]
  ]
}
