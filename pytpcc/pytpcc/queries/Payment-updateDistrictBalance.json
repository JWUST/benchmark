{
  "priority" : 1,
  "operators": {
    "load": {
       "type": "TableLoad",
       "table": "DISTRICT"
    },

    "idx_scan_D_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(d_id)d,
      "fields": ["D_ID"],
      "index": "DISTRICT_idx_D_ID"
    },

    "idx_scan_D_W_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(w_id)d,
      "fields": ["D_W_ID"],
      "index": "DISTRICT_idx_D_W_ID"
    },

    "intersect": {
      "type": "IntersectPositions"
    }, 

    "validate" : {
      "type" : "ValidatePositions"
    },
    "updateinc" : { 
        "type" : "PosUpdateIncrementScan",
        "column" : "D_YTD",
        "offset" : %(d_ytd)f
    },
    "NoOp": {
      "type": "NoOp"
    }
  },
  "edges": [
    ["load", "idx_scan_D_ID"], 
    ["load", "idx_scan_D_W_ID"],

    ["idx_scan_D_ID", "intersect"], 
    ["idx_scan_D_W_ID", "intersect"],

    ["intersect", "validate"],
    ["validate", "updateinc"],
    ["updateinc","NoOp"]
  ]
}
