{
  "priority" : 1,
  "operators": {
    "load": {
       "type": "TableLoad",
       "table": "ORDERS"
    },

    "idx_scan_O_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(no_o_id)d,
      "fields": ["O_ID"],
      "index": "ORDERS_idx_O_ID"
    },

    "idx_scan_O_D_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(d_id)d,
      "fields": ["O_D_ID"],
      "index": "ORDERS_idx_O_D_ID"
    },

    "idx_scan_O_W_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(w_id)d,
      "fields": ["O_W_ID"],
      "index": "ORDERS_idx_O_W_ID"
    },

    "intersect": {
      "type": "IntersectPositions"
    }, 

    "validate" : {
      "type" : "ValidatePositions"
    },
    "update": {
       "type": "PosUpdateScan",
       "data": {
          "O_CARRIER_ID": %(o_carrier_id)d
       }
    },
    "NoOp": {
      "type": "NoOp"
    }
  },
  "edges": [
    ["load", "idx_scan_O_ID"], 
    ["load", "idx_scan_O_D_ID"], 
    ["load", "idx_scan_O_W_ID"], 

    ["idx_scan_O_ID", "intersect"], 
    ["idx_scan_O_D_ID", "intersect"], 
    ["idx_scan_O_W_ID", "intersect"], 

    ["intersect", "validate"],
    ["validate", "update"],
    ["update","NoOp"]
  ]
}
