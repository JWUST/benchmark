{
  "priority" : 1,
  "operators": {
    "load": {
       "type": "TableLoad",
       "table": "CUSTOMER"
    },

    "idx_scan_C_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(c_id)d,
      "fields": ["C_ID"],
      "index": "CUSTOMER_idx_C_ID"
    },

    "idx_scan_C_D_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(c_d_id)d,
      "fields": ["C_D_ID"],
      "index": "CUSTOMER_idx_C_D_ID"
    },

    "idx_scan_C_W_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(c_w_id)d,
      "fields": ["C_W_ID"],
      "index": "CUSTOMER_idx_C_W_ID"
    },

    "intersect": {
      "type": "IntersectPositions"
    }, 

    "validate" : {
      "type" : "ValidatePositions"
    },
    "update": {
       "type": "PosUpdateScan",
       "data": {
         "C_BALANCE": %(c_balance)f,
         "C_YTD_PAYMENT": %(c_ytd_payment)f,
         "C_PAYMENT_CNT": %(c_payment_cnt)d
       }
    },
    "NoOp": {
      "type": "NoOp"
    }
  },
  "edges": [
    ["load", "idx_scan_C_ID"], 
    ["load", "idx_scan_C_D_ID"], 
    ["load", "idx_scan_C_W_ID"], 

    ["idx_scan_C_ID", "intersect"], 
    ["idx_scan_C_D_ID", "intersect"], 
    ["idx_scan_C_W_ID", "intersect"], 

    ["intersect", "validate"],
    ["validate", "update"],
    ["update","NoOp"]
  ]
}
