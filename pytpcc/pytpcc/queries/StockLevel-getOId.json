{
  "priority" : 1,
  "operators": {
    "load": {
       "type": "TableLoad",
       "table": "DISTRICT"
    },

    "idx_scan_D_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(d_id)d,
      "fields": ["D_ID"],
      "index": "DISTRICT_idx_D_ID"
    },

    "idx_scan_D_W_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(w_id)d,
      "fields": ["D_W_ID"],
      "index": "DISTRICT_idx_D_W_ID"
    },

    "intersect": {
      "type": "IntersectPositions"
    },

    "validate" : {
      "type" : "ValidatePositions"
    }, 
    "project": {
       "type": "ProjectionScan",
       "fields": ["D_NEXT_O_ID"]
    }
  },
  "edges": [
    ["load", "idx_scan_D_ID"], 
    ["load", "idx_scan_D_W_ID"],

    ["idx_scan_D_ID", "intersect"], 
    ["idx_scan_D_W_ID", "intersect"],

    ["intersect", "validate"],
    ["validate", "project"]
  ]
}
