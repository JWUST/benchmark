{
  "priority" : 1,
  "operators": {
    "load": {
       "type": "TableLoad",
       "table": "CUSTOMER"
    },

    "idx_scan_C_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(c_id)d,
      "fields": ["C_ID"],
      "index": "CUSTOMER_idx_C_ID"
    },

    "idx_scan_C_D_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(c_d_id)d,
      "fields": ["C_D_ID"],
      "index": "CUSTOMER_idx_C_D_ID"
    },

    "idx_scan_C_W_ID": {
      "type": "IndexAwareTableScan",
      "vtype": 0,
      "value": %(c_w_id)d,
      "fields": ["C_W_ID"],
      "index": "CUSTOMER_idx_C_W_ID"
    },

    "intersect": {
      "type": "IntersectPositions"
    }, 

    "validate" : {
      "type" : "ValidatePositions"
    },
    "project": {
       "type": "ProjectionScan",
       "fields": ["C_FIRST", "C_MIDDLE", "C_LAST", "C_STREET_1", "C_STREET_2", "C_CITY",
                  "C_STATE", "C_ZIP", "C_PHONE", "C_SINCE", "C_CREDIT", "C_CREDIT_LIM",
                  "C_DISCOUNT", "C_BALANCE", "C_YTD_PAYMENT","C_PAYMENT_CNT","C_DATA"]
    }
  },
  "edges": [
    ["load", "idx_scan_C_ID"], 
    ["load", "idx_scan_C_D_ID"], 
    ["load", "idx_scan_C_W_ID"], 

    ["idx_scan_C_ID", "intersect"], 
    ["idx_scan_C_D_ID", "intersect"], 
    ["idx_scan_C_W_ID", "intersect"], 

    ["intersect", "validate"],
    ["validate", "project"]
  ]
}
